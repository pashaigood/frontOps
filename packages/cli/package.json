    "start": "SOURCE=test-src OUTPUT=test-dist babel-node ./src/index.js",
    "start:dist": "SOURCE=test-src OUTPUT=test-dist node ./dist/index.js",
    "test": "NODE_PATH=./src SOURCE=test-src jest",
    "benchmark": "SOURCE=test-src OUTPUT=test-dist babel-node ./benchmark/",
    "benchmark:server": "SOURCE=test-src OUTPUT=test-dist babel-node ./benchmark/development/actoins/warmServer/test.js",
    "watch": "npm-watch",
    "storybook": "SOURCE=test-src OUTPUT=test-dist start-storybook -p 9001 -c ./dist/storybook"
  },
