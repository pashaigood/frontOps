{
  "name": "@FrontOps/webpack",
  "version": "1.0.0",
  "description": "Webpack bootsrap",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir lib -x \".ts\",\".tsx\",\".js\",\".jsx\",\".es6\",\".es\"",
    "start": "npm run build -- -w",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@webpack-blocks/core": "^1.0.0",
    "load-plugin": "^2.2.2",
    "webpack": "^4.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-typescript": "^7.0.0-beta.46",
    "array-unique": "^0.3.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.4.3",
    "fancy-log": "^1.3.2",
    "fast-glob": "^2.2.1",
    "findup-sync": "^2.0.0",
    "jest": "^22.4.3",
    "micromatch": "^3.1.10",
    "resolve": "^1.7.1"
  },
  "jest": {
    "modulePaths": ["<rootDir>/src"],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|js)x?$": "babel-jest"
    }
  }
}
